{
	"name": "eglobal/template-cache-bundle",
	"type": "symfony-bundle",
	"description": "Symfony template caching functionality.",
	"keywords": [ "template", "cache" ],
	"license": "MIT",
	"require": {
		"php": ">=7.0",
		"doctrine/annotations": "1.*",
		"sensio/framework-extra-bundle": "~3.0",
		"symfony/asset": "~2.1|~3.0",
		"symfony/assetic-bundle": "~2.1|~3.0",
		"symfony/framework-bundle": "~2.3|~3.0",
		"symfony/translation": "~2.3|~3.0",
		"symfony/twig-bundle": "~2.3|~3.0",
		"symfony/templating": "~2.3|~3.0",
		"psr/log": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "5.3.*",
		"matthiasnoback/symfony-config-test": "^1.4",
		"matthiasnoback/symfony-dependency-injection-test": "^0.7"
	},
	"autoload": {
		"psr-4": {
			"EGlobal\\Bundle\\TemplateCacheBundle\\": ""
		},
		"exclude-from-classmap": [
			"Tests/"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
